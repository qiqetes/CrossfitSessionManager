<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" styleClass="bg" stylesheets="@MyStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crossfitsessionmanager.FXMLAddTemplateController">
